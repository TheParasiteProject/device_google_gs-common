package {
    default_applicable_licenses: ["Android-Apache-2.0"],
}

cc_binary {
    name: "dump_mailbox",
    srcs: ["dump/dump_mailbox.cpp"],
    init_rc: ["init.mailbox.rc"],
    cflags: [
        "-Wall",
        "-Wextra",
        "-Werror",
    ],
    shared_libs: [
        "libbase",
        "libdump",
        "liblog",
    ],
    vendor: true,
    relative_install_path: "dump",
}
